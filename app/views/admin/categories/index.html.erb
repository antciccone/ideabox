<div class="container">
  <h1> List of Categories </h1>
  <p><%= link_to "Create New Category", new_admin_category_path %></p>
  <ol>
  <% @categories.each do |category| %></br>
  <li><%= link_to category.name, category_path(category) %></li>
  <%= link_to "Edit", edit_admin_category_path(category) %></br>
    <%= link_to "Delete", admin_category_path(category), method: :delete %></br>
<% end %>
</ol>
</div>
